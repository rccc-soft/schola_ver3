<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 外側の長方形（角丸） -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="24dp" />
            <stroke android:width="1dp" android:color="#79747E" />
        </shape>
    </item>

    <!-- 内側の小さい黄色の円 -->
    <item android:left="9dp" android:top="15dp" android:right="199dp" android:bottom="15dp">
        <shape android:shape="oval">
            <solid android:color="#000000" />
        </shape>
    </item>

    <item android:left="10dp" android:top="16dp" android:right="200dp" android:bottom="16dp">
        <shape android:shape="oval">
            <solid android:color="#FFFF00" />
        </shape>
    </item>
</layer-list>